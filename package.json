{
  "name": "vscode-babel-repl-patch",
  "displayName": "Babel REPL Patch",
  "description": "vscode-babel-repl的修补版本",
  "version": "0.1.3",
  "publisher": "Zhuzesen-js",
  "engines": {
    "vscode": "^1.7.0"
  },
  "license": "MIT",
  "categories": [
    "Languages",
    "Other"
  ],
  "galleryBanner": {
    "color": "#f6db47",
    "theme": "light"
  },
  "keywords": [
    "babel",
    "javascript",
    "es6",
    "repl",
    "transpiler"
  ],
  "icon": "icon.png",
  "bugs": {
    "url": "https://github.com/zesenzhu/vscode-babel-repl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zesenzhu/vscode-babel-repl.git"
  },
  "activationEvents": [],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.newBabelRepl",
        "title": "Open new Babel REPL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^7.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "mkdirp": "^0.5.1"
  }
}